{
    "root": true,
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "prettier", "plugin:prettier/recommended"],
    "plugins": ["react", "prettier"],
    "ignorePatterns": ["node_modules/"],
    "env": {
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off"
    }
}
